#include <stdio.h>
#include <stdlib.h>

struct node {
    int info;
    struct node *next;
};
struct node* insertend(struct node *start, int item) {
    struct node *p, *temp;
    p = (struct node*)malloc(sizeof(struct node));
    p->info = item;
    p->next = NULL;

    if (start == NULL) {
        start = p;
        return start;
    }

    temp = start;
    while (temp->next != NULL)
        temp = temp->next;

    temp->next = p;
    return start;
}

struct node* rev(struct node *start){
    struct node *cur=NULL,*temp=start->next;
    while(start!=NULL){
            temp=start->next;
    start->next=cur;
    cur=start;
    start=temp;
    }
    return cur;
}

struct node* deletelast(struct node *start) {
    struct node *temp, *follow;

    if (start == NULL) {
        printf("List is empty.\n");
        return start;
    }

    // Only one node
    if (start->next == NULL) {
        free(start);
        return NULL;
    }

    temp = start;
    while (temp->next != NULL) {
        follow = temp;
        temp = temp->next;
    }

    follow->next = NULL;
    free(temp);

    return start;
}

void display(struct node *start) {
    struct node *p = start;
    
    if (p == NULL) {
        printf("Stack is empty.\n");
        return;
    }
    p=rev(start);
    while (p != NULL) {
        printf("%d\n", p->info);
        p = p->next;
    }
}

int main() {
    struct node *start = NULL;
    int n=0, item;
    printf("1.Push\n2.Pop\n3.Display\n4.Exit\n");
    while(n!=4){
            scanf("%d",&n);
        switch(n){
    case 1:
        printf("Enter element to be inserted\n");
        scanf("%d",&item);
        start=insertend(start,item);
        break;
    case 2:
        start=deletelast(start);
        break;
    case 3:
        display(start);
        break;
        }
    }
}
