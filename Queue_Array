#include<stdio.h>
#define maxsize 5
int q[maxsize];
int front=-1,rear=-1;
void insert(int item)
{
	if(rear==maxsize-1)
	printf("Queue is full");
	else if(front==-1)
	{
		front=front+1;
		rear=rear+1;
		q[front]=item;
	}
	
	else
	{
		rear=rear+1;
		q[rear]=item;
	}
}
int delete()
{
	int item;
	if(front==-1)
	{
	printf("Queue is empty\n");
	return 0;
	}
	
	else if(front==rear)
	{
		item=q[front];
		front=-1;rear=-1;
		return item;
	}
	else{
		item=q[front];
		front=front+1;
		return item;
	}
}
void display()
{
	int i;
	if(front==-1)
	printf("Queue is empty\n");
	else{
		for(i=front;i<=rear;i++)
		printf("%d\n",q[i]);
	}
}
void main()
{
	int n=0,item;
	while(n!=4)
	{
	printf("\nEnter 1 for insertion\n2 for deletion\n3 for display\n4 to exit\n");
	scanf("%d",&n);
	if(n==1)
	{
		printf("Enter the item to be inserted:\n");
		scanf("%d",&item);
		insert(item);
	}
	else if(n==2)
	{
		int x=delete();
		printf("Deletion successful\n");	
	}
	else if(n==3)
	{
		display();
	}
}
}
